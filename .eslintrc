{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["prettier"],
  "globals": {
    "document": false,
    "window": false
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": ["error", { "packageDir": "." }],
    "prettier/prettier": "error"
  }
 }